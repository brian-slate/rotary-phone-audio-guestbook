[Unit]
Description=Boot LED Animation - Amber breathing while system starts
DefaultDependencies=no
Before=basic.target
After=local-fs.target systemd-modules-load.service

[Service]
Type=simple
ExecStart=/usr/bin/python3 /home/admin/rotary-phone-audio-guestbook/src/bootLed.py
Restart=no
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=sysinit.target
